    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "asaak_full",
            "type": "python",
            "request": "launch",
            "module": "run_job",
            "args": [
                "execute",
                "--job_name=asaak",
                "--full_refresh=yes"
            ]
        },
        {
            "name": "asaak",
            "type": "python",
            "request": "launch",
            "module": "run_job",
            "args": [
                "execute",
                "--job_name=asaak",
                "--full_refresh=no"
            ]
        },
    ]
}
